import java.util.*;
class Alphabet
{
public static void main(String args[])
{
int n;
Scanner sc=new Scanner(System.in);
System.out.println("Enter n");
n=sc.nextInt();
char ch='A';
for(int i=1;i<=n;i++)
{
for(int j=1;j<=i;j++)
{
System.out.print(ch + "  ");
ch++;
}
System.out.println();
}
}
}

OUTPUT:
Enter n
5
A
B  C
D  E  F
G  H  I  J
K  L  M  N  O
